import csv
import os
import sys

CSRFILE = 'sales.csv'

infile = open(CSRFILE, 'r', newline = "")

reader = csv.reader(infile)


for row in reader:
    i = 0
    cust_id = row[0]
    subtotal = row[3]
    tax = row[4]
    freight = row[5]
    i += 1

    print(
        format(cust_id, '<10'),
        format(subtotal, '<14')
    )