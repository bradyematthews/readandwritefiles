import csv
import os
import sys

#File Names
CUSTFILE = 'customers.csv'

infile = open(CUSTFILE, 'r', newline = "")

reader = csv.reader(infile)



for row in reader:
    i = 0
    first_name = row[1]
    last_name = row[2]
    country = row[4]
    i += 1

    print(format(first_name, '<14s'), format(last_name, '<16s'), format(country, '<14s'))


